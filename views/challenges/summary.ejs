    <p>
        <datetime class="timeDetails"><%= challengerSchema.timelimit%></datetime>
        <a href="/home">
            <button class="buttonowner">                             
                <img src="public/owner.png" alt="Owner ">
            </button>
        </a>        
    </p>
    <p class="textDetails"><%= challengerSchema.description.substring (0,500) + ' ...'%></p>

    <a href="/search">
