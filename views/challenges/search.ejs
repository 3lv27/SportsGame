
<div id="map"></div>
<ul>
    <% challenger.forEach((challenge) => { %>
        <ul class="challengerList">
            <div class="containerList">
                <div class="mapList">
                    <%= challenge.location%>
                </div>
                <h3 class="tittleList">
                    <%=challenge.challengeName%>
                </h3>
                <p class="textList">
                    <%= challenge.description%>
                </p>
                <p>
                    <a class="buttonOwnerList" href="/resume/<%= challenge.owner %>">
                        <button class="iconOwner">
                            <img src="public/owner.png" alt="Owner ">
                        </button>
                    </a>

                    <a href="/challenges/<%= challenge._id %>">
                        <button class="buttonSummaryList">Summary</button>
                    </a>
                    <datetime>
                        <%= challenge.timeLimit%>
                    </datetime>
                    <%= challenge.sport%>
                </p>
            </div>
        </ul>
        <% }); %>
</ul>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCIoSi4O6j2YmTJQFpyKg7qWHzq3zFs7UA"></script>
<script type="text/javascript">
    var challenges = <%- JSON.stringify(challenger) %>;
</script>
<script type="text/javascript" src="/javascripts/main-map.js"></script>